{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"requirePragma": false,
	"semi": false,
	"singleQuote": true,
	"printWidth": 80,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": true,
  
	"plugins": ["prettier-plugin-tailwindcss"]
}
